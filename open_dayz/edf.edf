	</element>
	<element name="nozombiearea" friendlyname="Zombie Free Area">
		<data name="positionA" type="coord3d" description="XYZ position" default="0,0,0" />
		<data name="positionB" type="coord3d" description="XYZ position" default="0,0,0" />
		<data name="interior" type="integer" description="GTA interior world" default="0" />
